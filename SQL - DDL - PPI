
  CREATE TABLE "SYSTEM"."USERS" 
   (	"ID" NUMBER(12,0) NOT NULL ENABLE, 
	"PROFILEIMAGE" VARCHAR2(60 BYTE) NOT NULL ENABLE, 
	"USERNAME" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"TYPEUSER" NUMBER(12,0) NOT NULL ENABLE, 
	"NAME" VARCHAR2(60 BYTE) NOT NULL ENABLE, 
	"EMAIL" VARCHAR2(60 BYTE) NOT NULL ENABLE, 
	"PASSWORD" VARCHAR2(60 BYTE) NOT NULL ENABLE, 
	"PHONENUMBER" VARCHAR2(12 BYTE) NOT NULL ENABLE, 
	"GENDER" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	"BIRTHDATE" DATE NOT NULL ENABLE, 
	"COUNTRY" VARCHAR2(30 BYTE) NOT NULL ENABLE, 
	 CONSTRAINT "USERS_PK" PRIMARY KEY ("ID")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
